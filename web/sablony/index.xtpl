        <div class="infopruh_odpocet"><span id="odpocetDny"></span>&nbsp;dní <div class="infopruh_lomitko"></div> <span id="odpocetHodiny"></span>&nbsp;hodin <div class="infopruh_lomitko"></div><span id="odpocetMinuty"></span>&nbsp;minut</div>
      </div>
      <script>
      odpocet();
      </script>
    </div>

    <!--                 ZÁKLADNÍ INFO                     -->
