<!-- begin:novinky -->

<div class="container sirka-1200 mt-3">
  <div class="row mt-7">
    <div class="col-12 text-center">
      <div class="h6">co je nového? krátce a stručně</div>
      <div class="h1 red mt-3 mb-6">Novinky z Gameconu</div>
    </div>
  </div>
  <div class="grid">
    <!-- begin:novinka -->
    <div class="grid-item box-360">
      <!-- begin:SObrazkem -->
      <div class="aktivita_obrazek" style="background-image: url({novinka.obrazek})"></div>
      <div class="box_ohraniceni">
        <div class="box_obsah">
      <!-- end:SObrazkem -->
      <!-- begin:BezObrazku -->
      <div class="box_ohraniceni box_ohraniceni-bezObrazku">
        <div class="box_obsah box_obsah-bezObrazku">
      <!-- end:BezObrazku -->
          <div class="pl-30px pr-30px">
            <div class="novinka_datum">{novinka.datum}</div>
            <div class="novinka_nadpis">{novinka.nazev}</div>
            <div class="novinka_popis">{novinka.hlavniText}</div>
          </div>
        </div>
      </div>
    </div>
    <!-- end:novinka -->
  </div>
</div>

<script>
displayMasonry('.grid');
</script>

<p style="text-align:center">
  <!-- begin:novejsi --> <a href="{url}">« novější</a> &emsp;&emsp; <!-- end:novejsi -->
  <!-- begin:starsi --> <a href="{url}">starší »</a> <!-- end:starsi -->
</p>

<!-- end:novinky -->
