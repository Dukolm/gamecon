{
  "name": "gamecon-cz/gamecon",
  "type": "project",
  "description": "Počítačem řízené zázemí pro Největší festival nepočítačových her",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/godric-cz/xtemplate"
    },
    {
      "type": "vcs",
      "url": "https://github.com/godric-cz/db-backup"
    },
    {
      "type": "vcs",
      "url": "https://github.com/godric-cz/db-test"
    },
    {
      "type": "vcs",
      "url": "https://github.com/godric-cz/db-migrations"
    }
  ],
  "require": {
    "php": ">=7.3",
    "ext-json": "*",
    "ext-intl": "*",
    "ext-mysqli": "*",
    "ext-mbstring": "*",
    "ext-pdo": "*",
    "michelf/php-markdown": "~1.4",
    "dg/mysql-dump": "^1.4",
    "tracy/tracy": "~2.4",
    "godric/xtemplate": "dev-master",
    "roderik/pwgen-php": "^0.1.7",
    "godric/db-backup": "dev-master",
    "defuse/php-encryption": "^2.1",
    "godric/db-migrations": "dev-master",
    "setasign/tfpdf": "^1.25",
    "wikimedia/less.php": "^3.0",
    "doctrine/orm": "*"
  },
  "require-dev": {
    "dg/ftp-deployment": "~2.7",
    "phpunit/phpunit": "^9.5",
    "roave/security-advisories": "dev-master"
  },
  "autoload-dev": {
    "psr-4": {
      "Gamecon\\Tests\\": "tests"
    }
  },
  "config": {
    "sort-packages": true
  }
}
